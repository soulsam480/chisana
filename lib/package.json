{
  "name": "chisana",
  "version": "0.0.2-alpha",
  "description": "Jotai inspired atomic state management for vue 3",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/chisana.cjs.js",
      "import": "./dist/chisana.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/chisana.cjs.js",
  "module": "./dist/chisana.es.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && npm run dts",
    "dts": "tsc --emitDeclarationOnly --declaration"
  },
  "author": "soulsam480 <soulsam480@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "4.4.4",
    "vite": "^2.6.14",
    "vue": "^3.2.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulsam480/chisana.git"
  }
}
