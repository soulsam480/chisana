{
  "name": "chisana",
  "version": "0.0.2-alpha",
  "description": "Jotai inspired atomic state management for vue 3",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "require": "./dist/utils/utils.js",
      "import": "./dist/utils/utils.mjs",
      "types": "./dist/utils/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.mjs"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "vite build && npm run dts",
    "dts": "tsc --emitDeclarationOnly --declaration"
  },
  "author": "soulsam480 <soulsam480@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "4.5.4",
    "vite": "^2.7.13",
    "vue": "^3.2.29"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulsam480/chisana.git"
  }
}
